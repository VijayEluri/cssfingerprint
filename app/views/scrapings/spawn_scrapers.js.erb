<%= update_page do |page|
	effective_threads.times do |i|
		page.insert_html :bottom, 'scrapers', "<iframe style='float: left; width: 1px; height: 1px; border: none;' 
				src='#{ autoscrape_visitations_url(:thread_id => i) }'>" # , :host => "#{i}.cssfingerprint.com" 
	end
	page.assign 'completed', false
end %>