<%= update_page do |page|
	effective_threads.times do |i|
		page.insert_html :bottom, 'scrapers', "<iframe id='scraper_thread_#{ i }' style='float: left; height: 1.5em; clear:both; margin: 0; buffer: 0; border: none; overflow: hidden;' 
				src='#{ autoscrape_visitations_url(:thread_id => i, :host => i.to_s + '.cssfingerprint.com') }'>" 
	end
	page.assign 'completed', false
end %>